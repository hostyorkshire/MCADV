[MASTER]
init-hook='import sys; sys.path.append(".")'
ignore=CVS,.git,__pycache__,.venv,venv,env,ENV,build,dist,logs

[MESSAGES CONTROL]
disable=
    C0111,
    C0103,
    R0913,
    R0914,
    W0703,
    R0912,
    R0915

[FORMAT]
max-line-length=120
indent-string='    '

[BASIC]
good-names=i,j,k,ex,Run,_,e,f,fd,ip

[DESIGN]
max-args=10
max-locals=20
max-returns=6
max-branches=15
max-statements=60
